<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  <mapper namespace="StudentMapper">
  	<!--Insert id = joinMember, joinStudent  -->
  	<insert id="joinMember" parameterType="com.choa.member.student.StudentDTO">
  		insert into member values(#{id}, #{pw}, #{name}, #{age}, #{grade}, #{filename}, #{oriname})
  	</insert>
  	<insert id="joinStudent" parameterType="com.choa.member.student.StudentDTO">
  		insert into student values(#{sid}, #{tid}, #{phone})
  	</insert>
  
  </mapper>